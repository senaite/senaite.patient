<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
        cookie_persistence="False" default_skin="Plone Default"
        request_varname="plone_skin">

  <!-- senaite.patient templates -->
  <object name="senaite_patient_templates" meta_type="Filesystem Directory View"
          directory="senaite.patient:skins/templates"
          remove="True" />

  <skin-path name="*">
    <layer name="senaite_patient_templates" insert-after="custom" remove="True" />
  </skin-path>
</object>
